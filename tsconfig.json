{
  "compilerOptions": {
    "lib": ["es2022", "dom"],
    "skipLibCheck": true,
    "allowJs": true,
    "noImplicitAny": false,
    "checkJs": true,
    "strict": true,
    "noEmit": true,
    "moduleResolution": "Node",
    "module": "ESNext",
    "esModuleInterop": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true
  },
  "include": [
    "**/*"
  ],
}